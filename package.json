{
  "name": "crud-road-tp-senior",
  "description": "CRUD Road To Senior",
  "version": "1.0.0",
  "private": true,
  "author": "Yusuf Bagus Anggara",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "cypress": "cypress open"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@builder.io/partytown": "0.5.2",
    "@fontsource/poppins": "4.5.5",
    "@react-oauth/google": "0.2.6",
    "antd": "^4.21.0",
    "axios": "0.26.1",
    "clsx": "1.1.1",
    "dayjs": "1.11.0",
    "i18next": "21.6.14",
    "next": "12.2.0",
    "next-plugin-antd-less": "1.8.0",
    "next-pwa": "5.4.7",
    "query-string": "7.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "3.1.4",
    "react-i18next": "11.16.2",
    "react-infinite-scroll-component": "6.1.0",
    "react-query": "3.34.19",
    "react-quick-pinch-zoom": "4.4.1",
    "tua-body-scroll-lock": "1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@cypress/code-coverage": "3.10.0",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/line-clamp": "0.4.0",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "14.0.4",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/node": "16.11.26",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.17.0",
    "autoprefixer": "10.4.4",
    "babel-jest": "27.5.1",
    "babel-plugin-import": "1.13.3",
    "commitizen": "4.2.4",
    "config": "3.3.7",
    "cssnano": "5.1.7",
    "cypress": "10.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.0",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-test-id": "2.1.0",
    "eslint-plugin-test-selectors": "2.0.3",
    "eslint-plugin-testing-library": "5.2.1",
    "eslint-plugin-crud-road-to-senior-web": "file:./lint_rules",
    "husky": "^7.0.0",
    "jest": "27.5.1",
    "less": "4.1.2",
    "less-loader": "10.2.0",
    "less-vars-to-js": "1.3.0",
    "lint-staged": "12.3.7",
    "next-compose-plugins": "2.2.1",
    "next-with-less": "2.0.5",
    "postcss": "8.4.12",
    "postcss-preset-env": "7.4.3",
    "prettier": "2.6.1",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.3"
  }
}
